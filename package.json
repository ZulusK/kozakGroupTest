{
  "name": "kozakGroupTest-be",
  "version": "0.0.1",
  "description": "Test task for Kozak Group",
  "main": "./index.js",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "debug": "cross-env NODE_ENV=development node --nolazy --inspect-brk=9229 index.js",
    "nodemon-debug": "cross-env NODE_ENV=development nodemon --debug --inspect-brk=9229 index.js",
    "lint": "esw *.js  . --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/_mocha --opts ./test/mocha.opts server",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --opts ./test/mocha.opts server",
    "test:coverage:ci": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --opts ./test/mocha.opts --report lcovonly server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ZulusK/kozakGroupTest-be.git"
  },
  "dependencies": {
    "bcrypt": "2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-fileupload": "0.4.0",
    "express-jwt": "5.3.1",
    "express-query-boolean": "2.0.0",
    "express-static-gzip": "0.3.2",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "google-libphonenumber": "^3.1.12",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "joi": "10.6.0",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "method-override": "^2.3.10",
    "moment": "2.22.2",
    "mongodb": "3.1.1",
    "mongoose": "5.2.5",
    "mongoose-paginate": "5.0.3",
    "morgan": "1.9.0",
    "nodemailer": "^4.6.8",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "pug": "^2.0.3",
    "query-string": "6.1.0",
    "validator": "10.4.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": ">=3.16.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "4.0.2",
    "faker": "^4.1.0",
    "flat": "^4.1.0",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "mocha-clean": "1.0.0",
    "nodemon": "^1.17.5",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
